      "bundleIdentifier": "com.app.NavigationApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.app.NavigationApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4cf391f6-be2d-4b48-93bb-1927fc6c190b"
      }
    }
  }
}
